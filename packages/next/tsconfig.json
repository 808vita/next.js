{
  "extends": "../../tsconfig-tsec.json",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "target": "ES2017",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "stripInternal": true,
    "verbatimModuleSyntax": true,
    "paths": {
      "next/dist/compiled/*": ["./node_modules/@types/*", "./node_modules/*"],
      "next/dist/compiled/babel/*": [
        "./node_modules/@types/babel__*",
        "./node_modules/@babel/*"
      ],
      "next/dist/client/app-call-server": ["./src/client/app-call-server"]
    }
  },
  "exclude": [
    "dist",
    "./*.d.ts",
    "future/*.d.ts",
    "image-types/global.d.ts",
    "compat/*.d.ts",
    "legacy/*.d.ts",
    "types/compiled.d.ts",
    "navigation-types/*.d.ts",
    "navigation-types/compat/*.d.ts",
    "experimental/**/*.d.ts"
  ]
}
